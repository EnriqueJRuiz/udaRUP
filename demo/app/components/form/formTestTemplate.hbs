						<input type="text" name="departamento.descEu" class="formulario_linea_input" size="30" id="departamento_descEu_multi" />
				    </div>
				</div>
			</fieldset>
		<input type="submit" value="Env&iacute;o multientidad" />
		</fieldset>
	</form>

